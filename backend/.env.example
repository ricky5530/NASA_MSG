# Hackathon environment variable template
# For production, copy this file as .env and fill in actual values

# OpenAI API KEY
OPENAI_API_KEY=your_openai_api_key_here

# Server port (automatically set on Render)
PORT=8000

# CORS 허용 도메인 (프론트엔드 URL)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://your-frontend.vercel.app

# Log Level
LOG_LEVEL=INFO